{
  "expo": {
    "name": "vaultvu",
    "slug": "vaultVu",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/vaultvu-logo.png",
    "scheme": "vaultvu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vaultvu-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.arpitsehal.vaultvu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6d713068-7443-4406-8312-3227fd224d1c",
        "slug": "vaultvu"
      }
    },
    "owner": "arpitsehal"
  }
}
